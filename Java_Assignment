Q.50 Create a linked list of 3 records about books and element type is Object of class Book. Book details are : bookid,booktitle,bookauthor,bookprice. Print all book
     details in reverse order.
 Ans:package Pro_Ass;

import java.util.ArrayList;
import java.util.Iterator;
import java.util.LinkedList;

class books
{
	String booksid,booksprice;
	String bookstitle,booksauthor;
	
	public books(String booksid,String booksprice, String bookstitle, String booksauthor) {
		this.booksid = booksid;
		this.booksprice = booksprice;
		this.bookstitle = bookstitle;
		this.booksauthor = booksauthor;
	}
}
public class Ass_50 {

	public static void convertToLinkedList(ArrayList<books> al,LinkedList<String> list)

	{
		String str;
		for(books c:al)
		{
			//System.out.println("Book ID : "+c.booksid+" Book Title : "+c.bookstitle+" Book by : "+c.booksauthor+" Price : "+c.booksprice);
			str="Book ID : "+c.booksid+" Book Title : "+c.bookstitle+" Book by : "+c.booksauthor+" Price  "+c.booksprice;
			list.add(str);
		}
	}
	
	public static void display(LinkedList<String> l)
	{
		Iterator<String> it=l.iterator();
		System.out.println("Nodes in Linked List is :");
		while(it.hasNext())
		{
			System.out.print(it.next()+" ");
			System.out.println();
		}
	}
	public static void main(String[] args) {
		
		ArrayList<books> al=new ArrayList<books>();
		
		al.add(new books("101","2500","Ajaya: Roll of the Dice & Rise of Kali - Duryodhana's Mahabharata", "Anand Neelakantan"));  
		al.add(new books("102","4000", "The Stranger Trilogy", "Novoneel Chakraborty"));    //Adding object of customer class
		al.add(new books("103","980", "The Book Thief", "Markus Zusak"));
		al.add(new books("104","670", "Asura: Tale of the Vanquished", "Anand Neelakantan"));
		
		 LinkedList<String> list = new LinkedList<String>();

		convertToLinkedList(al,list);
		
		display(list);
	}
}
