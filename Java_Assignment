Q.48 Create a List A which contains 5 strings .Create a List B where length of all strings from List A should be stored.Create 
 Ans:
 package Pro_Ass;

import java.util.ArrayList;
import java.util.Iterator;

public class Ass_48 {
	public static void add(ArrayList<String> l,String item)
	{
		l.add(item);          
	}
	public static void add(ArrayList<Integer> l,int item)
	{
		l.add(item);          
	}
	public static void display(ArrayList<String> l)
	{
		int i=1;
		Iterator<String> it=l.iterator();
		
		System.out.print("\nArray List is : ");
		System.out.println();
		while(it.hasNext())
		{
			System.out.println(i+") "+it.next());
			i++;
		}
	}
	public static void display_num(ArrayList<Integer> l)
	{
		int i=1;
		Iterator<Integer> it=l.iterator();
		
		System.out.print("\nArray List is : ");
		System.out.println();
		while(it.hasNext())
		{
			System.out.println(i+") "+it.next());
			i++;
		}
	}
	public static void copyArray(ArrayList<String> l1,ArrayList<Integer> l2)
	{
		int num;
		String name;
		Iterator<String> it=l1.iterator();
		
		while(it.hasNext())
		{
			name=it.next();	
			num=name.length();
			add(l2,num);
		}
	}

	public static void main(String[] args) {
		 ArrayList<String> city=new ArrayList<String>();
			
			add(city,"Pune");
			add(city,"London");
			add(city,"Tokyo");
			add(city,"Barcelona");
			add(city,"New York City");
			display(city);
			
			ArrayList<Integer> num=new ArrayList<Integer>();
			copyArray(city,num);
			
			display_num(num);

	}
}
