Q.44 Create an ArrayList of 5 integers and display the average of all numbers.
 Ans:
 package Pro_Ass;

import java.util.ArrayList;
import java.util.Iterator;

public class Ass_44 {
	public static void add(ArrayList<Integer> l,int item)
	{
		l.add(item);          
	}
	public static void display(ArrayList<Integer> l)
	{
		Iterator<Integer> it=l.iterator();
		
		System.out.print("\nArray List is : ");
		System.out.println();
		while(it.hasNext())
		{
			System.out.println(it.next()+" ");
			
		}
	}
	public static void Avg(ArrayList<Integer> l)
	{
		int sum=0,avg,i=1;
		Iterator<Integer> it=l.iterator();
		
		while(it.hasNext())
		{
			//System.out.println(it.next());
			sum=sum+it.next();
			i++;
		}
		avg=sum/i;
		System.out.println("Average of all numbers : "+avg);
	}
	

	public static void main(String[] args) {
      ArrayList<Integer> li=new ArrayList<Integer>();
		
		add(li,10);
		add(li,20);
		add(li,30);
		add(li,40);
		add(li,50);
		display(li);
		System.out.println("--------------Average--------------");
		Avg(li);
		System.out.println("-----------------------------------");
		

	}
}
