     showFactorial().
 Ans:
 package Pro_Ass;
final class Factorial_Ass_36
{
	public int sum;

	public int getFactorial( int a)
	{
		sum=1;
		for(int i=2;i<(a+1);i++)
		{
			sum=sum*i;		
		}
		System.out.println("The Factorial of "+a+" is : "+sum);
		return sum;
	}
}

class showFactorial_Ass36
{
	int sum;
	Factorial_Ass_36 obj=new Factorial_Ass_36();
	void showFactorial(int a)
	{
		sum=obj.getFactorial(a);
	}
}
public class Ass_36 {

	public static void main(String[] args) {
		showFactorial_Ass36  obj=new showFactorial_Ass36();
		
		obj.showFactorial(7);

	}

