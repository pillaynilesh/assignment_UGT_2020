Q.46 Make an ArrayList of five city names and search a specific city and display city name with position in ArrayList.
 Ans:
 package Pro_Ass;

import java.util.ArrayList;
import java.util.Iterator;
import java.util.Scanner;

public class Ass_46 {
	private static Scanner s;
	
	public static void add(ArrayList<String> l,String item)
	{
		l.add(item);          
	}
	public static void display(ArrayList<String> l)
	{
		int i=1;
		Iterator<String> it=l.iterator();
		
		System.out.print("\nArray List is : ");
		System.out.println();
		while(it.hasNext())
		{
			System.out.println(i+") "+it.next());
			i++;
		}
	}
	public static void search(ArrayList<String> l)
	{
		String name,cityName;
		int i=0;
		s = new Scanner(System.in);
		System.out.println("Enter City to be Search :");
		name=s.next();
	    
		Iterator<String> it=l.iterator();
		while(it.hasNext())
		{
			cityName=it.next();
			if(cityName.equalsIgnoreCase(name)) 
			{
			System.out.println(" "+cityName+"  having Index "+i);
			break;
		    }
			else if(cityName!=name)
				System.out.println("Invaild");
			i++;
		}
		
	}

	public static void main(String[] args) {
	      ArrayList<String> city=new ArrayList<String>();
			
			add(city,"Pune");
			add(city,"London");
			add(city,"Tokyo");
			add(city,"Barcelona");
			add(city,"New York City");
			display(city);
			search(city);
			
			

	}
}
